{
  "name": "hubspot-integration-up",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:date-formatter": "jest --testPathIgnorePatterns=/integration/",
    "hubspot:dev:register": "node scripts/register-workflow-action.mjs date-formatter register dev",
    "hubspot:dev:update": "node scripts/update-workflow-action.mjs date-formatter dev",
    "hubspot:dev:list": "node scripts/list-workflow-actions.mjs date-formatter dev",
    "hubspot:dev:delete": "node scripts/delete-workflow-action.mjs date-formatter dev",
    "hubspot:prod:register": "node scripts/register-workflow-action.mjs date-formatter register prod",
    "hubspot:prod:update": "node scripts/update-workflow-action.mjs date-formatter prod",
    "hubspot:prod:list": "node scripts/list-workflow-actions.mjs date-formatter prod",
    "hubspot:prod:delete": "node scripts/delete-workflow-action.mjs date-formatter prod",
    "usage:aggregate": "node scripts/aggregate-usage.mjs",
    "usage:aggregate:dev": "NODE_ENV=development node scripts/aggregate-usage.mjs",
    "usage:aggregate:prod": "NODE_ENV=production node scripts/aggregate-usage.mjs",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules/.cache .turbo .swc"
  },
  "dependencies": {
    "@hubspot/api-client": "^13.0.0",
    "@supabase/supabase-js": "^2.52.1",
    "dotenv": "^17.2.0",
    "next": "15.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-jest": "^29.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "node-mocks-http": "^1.17.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
