{
  "name": "hubspot-integration-up",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:date-formatter": "jest --testPathIgnorePatterns=/integration/",
    "hubspot": "node scripts/hubspot-app.mjs",
    "hubspot:date-formatter:register:dev": "node scripts/hubspot-app.mjs register date-formatter dev",
    "hubspot:date-formatter:register:prod": "node scripts/hubspot-app.mjs register date-formatter prod",
